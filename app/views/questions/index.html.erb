<ul>
	<% @questions.each do |question| %>
		<li>
			<div class="question_info">
				<div class="stat-list">
					<div class="stats">
						<div class="votes-circle">14</div>
						<span class="stat-label"><p>votes<p><span>
					</div>
					<div class="stats">
						<div class="answers-circle">14</div>
						<span class="stat-label"><p>Answers<p><span>
					</div>
				</div>
				<div class="question-body">
					<div class="question-title">
						<%= link_to question.title, question %>
					</div>
					<div class="info">
						<ul class="tags">
	        		<li><a href="#">javascript</a></li>
	    			</ul>	
	    		</div>
	    	</div>
			</div>
		</li>
	<% end %>
</ul>

